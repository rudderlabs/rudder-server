{
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier", "plugin:import/errors", "plugin:import/warnings"],
  "rules": {
    "jsx-wrap-multiline": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false
  },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "src/"]
      }
    }
  }
}
