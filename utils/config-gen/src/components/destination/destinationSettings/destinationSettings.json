{
  "GA": [
    {
      "title": "1. Connection Settings",
      "fields": [
        {
          "type": "textInput",
          "label": "Tracking ID",
          "value": "trackingID",
          "regex": "^(UA|YT|MO)-\\d+-\\d+$",
          "required": true,
          "placeholder": "e.g: UA-125934xxx-1"
        }
      ]
    },
    {
      "title": "2. Pages and Screens",
      "fields": [
        {
          "type": "checkbox",
          "label": "Track all pages to Google Analytics",
          "value": "trackAllPages",
          "default": true
        },
        {
          "type": "checkbox",
          "label": "Track all screens",
          "value": "trackAllScreens"
        },
        {
          "type": "checkbox",
          "label": "Track categorized pages to Google Analytics",
          "value": "trackCategorizedPages",
          "default": true
        }
      ]
    },
    {
      "title": "3. Native SDK",
      "fields": [
        {
          "type": "checkbox",
          "label": "Use native SDK to send events",
          "value": "useNativeSDK",
          "default": false
        }
      ]
    },
    {
      "title": "4. Configure Custom Dimension",
      "fields": [
        {
          "type": "dynamicForm",
          "labelLeft": "Event Type",
          "labelRight": "Custom Dimension",
          "keyLeft": "from",
          "keyRight": "to",
          "placeholderLeft": "e.g: product_purchased",
          "placeholderRight": "e.g: product_price",
          "value": "customMappings"
        }
      ]
    }
  ],
